 frame\"})})]})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"p\",{style:{fontWeight:\"700\",fontSize:\"20px\",fontFamily:\"poppins\"},children:\"Share:\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"hr\",{style:{width:\"80%\",border:\"3px solid\",backgroundColor:\"black\",opacity:\"1\",marginTop:\"-3%\"}}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{class:\"row\",children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{class:\"col-4 col-md-4\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{href:\"https://www.facebook.com/sharer.php?u=https://cutoutwiz.com/5-important-photography-tips-for-beginners/\",target:\"_blank\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"img\",{src:\"../../assets/images/facebookblack.png\",height:\"100%\",width:\"50%\",alt:\"\"})})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{class:\"col-4 col-md-4\",children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"a\",{href:\"https://twitter.com/intent/tweet?text=%20https://cutoutwiz.com/5-important-photography-tips-for-beginners/\",target:\"_blank\",children:[\" \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"img\",{src:\"../../assets/images/twit.png\",height:\"100%\",width:\"50%\",alt:\"\"})]})}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{class:\"col-4 col-md-4\",children:/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"a\",{href:\"    https://www.linkedin.com/uas/login?session_redirect=https%3A%2F%2Fwww.linkedin.com%2FshareArticle%3Fmini%3Dtrue%26url%3Dhttps%3A%2F%2Fcutoutwiz.com%2F5-important-photography-tips-for-beginners%2F%26title%3D5%2520Important%2520Photography%2520Tips%2520for%2520Beginners%2520%25E2%2580%2593%2520CutOutWiz%26summary%3D%26source%3Dhttps%3A%2F%2Fcutoutwiz.com%2F5-important-photography-tips-for-beginners%2F\",target:\"_blank\",children:[\" \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"img\",{src:\"../../assets/images/linkdn.png